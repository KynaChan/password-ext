{
    "manifest_version":2 ,
    "name": "CatLocker",
    "version": "1.0.0",
    "description": "Your personal account management system, password tester and generator. Here in CatLocker! ",
    "browser_action" : {
        "default_icon" : "icon/image3.png",
        "default_popup": "index.html"
    },
    "icons": {
        "16": "icon/image3.png",
        "32": "icon/image3.png",
        "48": "icon/image3.png",
        "128":"icon/image3.png"
       },
       "permissions":[
           "activeTab",
           "tabs"
       ],
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js":[
                "script/main.js",
                "script/dashboard.js",
                "script/generator.js",
                "script/home.js",
                "script/libs/jquery.js"
            ],

            "css":["style.css"]
        }
    ]  
}