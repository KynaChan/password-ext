{
    "manifest_version":2 ,
    "name": "PW manager",
    "version": "1.0.0",
    "description": "Save time by managing your accounts and passwords here!",
    "browser_action" : {
        "default_icon" : "icon/images.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icon/images.png",
        "32": "icon/images.png",
        "48": "icon/images.png",
        "128":"icon/images.png"
       },
       "permissions":[
           "activeTab",
           "tabs"
       ],
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js":[
                "content.js",
                "libs/jquery.js"
            ],

            "css":["style.css"]
        }
    ]  
}